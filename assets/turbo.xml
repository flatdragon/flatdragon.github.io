<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:yandex="http://news.yandex.ru"
     xmlns:media="http://search.yahoo.com/mrss/"
     xmlns:turbo="http://turbo.yandex.ru"
     version="2.0">
  <channel>
    <item turbo="true">
      <link>{{ link }}</link>
      <turbo:content>
        <![CDATA[
          <header>
            <h1>{{ title }}</h1>
          </header>
          {{ content }}
        ]]>
      </turbo:content>
    </item>
  </channel>
</rss>
